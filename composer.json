{
    "name": "kareembelgharbi/php-sortedlinkedlist",
    "description": "A sorted linked list implementation in PHP",
    "type": "library",
    "keywords": ["linked-list", "sorted", "data-structure"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SortedLinkedList\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse",
        "phpcs": "phpcs",
        "phpcbf": "phpcbf",
        "check": [
            "@phpstan",
            "@phpcs",
            "@test"
        ]
    }
}
